{
  "name": "nextjs-latest-starter",
  "version": "0.3.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:tsuki-lab/nextjs-latest-starter.git"
  },
  "author": "hanetsuki <me@tsuki-lab.net> (https://github.com/tsuki-lab)",
  "license": "MIT",
  "scripts": {
    "dev": "next -p 3001",
    "dev:next": "next dev",
    "dev:path": "yarn pathpida -w",
    "build": "yarn pathpida && next build",
    "start": "next start",
    "pathpida": "pathpida -p .gitignore -o ./src/lib -s",
    "lint": "next lint --dir src",
    "stylelint": "stylelint --ignore-path .gitignore './src/**/*.{css,scss,ts,tsx}'",
    "fix": "yarn lint --fix && yarn stylelint --fix && yarn format",
    "typecheck": "tsc",
    "prettier": "prettier --config .prettierrc.json './**/*.{ts,tsx,json,md}'",
    "format": "yarn prettier --write",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.9",
    "@chakra-ui/react": "^2.2.9",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@types/js-cookie": "^3.0.2",
    "axios": "^0.27.2",
    "axios-case-converter": "^0.9.0",
    "framer-motion": "^6",
    "js-cookie": "^3.0.1",
    "next": "12.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ress": "^5.0.2"
  },
  "devDependencies": {
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/node": "18.0.5",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "12.2.2",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.18.1",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.7.1",
    "stylelint": "^14.9.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "4.7.4"
  }
}
